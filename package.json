{
  "name": "celoterminal",
  "productName": "CeloTerminal",
  "version": "1.0.1",
  "description": "Celo Terminal: One-stop-shop to interact with the decentralized apps in the Celo ecosystem.",
  "main": ".webpack/main",
  "scripts": {
    "start": "npx cross-env CELOTERMINAL_NETWORK_ID=62320 CELOTERMINAL_ACCOUNTS_DB=home/.celoterminal/celoaccounts-test.db npx electron-forge start",
    "mainnet": "npx cross-env CELOTERMINAL_ACCOUNTS_DB=home/.celoterminal/celoaccounts-test.db npx electron-forge start",
    "build": "npx env-cmd -f ./.publish.env electron-forge publish --dry-run",
    "publish": "npx env-cmd -f ./.publish.env electron-forge publish --from-dry-run",
    "lint": "eslint --ext .ts ."
  },
  "keywords": [],
  "author": {
    "name": "Zviad Metreveli",
    "email": "thezviad@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": "./forge.config.js"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "@electron-forge/plugin-webpack": "6.0.0-beta.54",
    "@electron-forge/publisher-github": "^6.0.0-beta.54",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@types/better-sqlite3": "^5.4.1",
    "@types/node-hid": "^1.3.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "cross-env": "^7.0.3",
    "css-loader": "^4.2.1",
    "electron": "11.2.0",
    "env-cmd": "^10.1.0",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "fork-ts-checker-webpack-plugin": "^5.0.14",
    "hook-shell-script-webpack-plugin": "^0.1.3",
